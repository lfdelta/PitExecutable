         x += sign(xVelocity);
   xVelocity = 0;
}

if (place_meeting(x, y+yVelocity, obj_player_collide)) {
   while (!place_meeting(x, y+sign(yVelocity), obj_player_collide))
         y += sign(yVelocity);
   yVelocity = 0;
}



//Move player
x += xVelocity;
y += yVelocity;
